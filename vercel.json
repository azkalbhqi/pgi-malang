{
    "builds": [
      {
        "src": "artisan",
        "use": "@vercel/php"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/api/index.php" },
      {
        "src": "/img/(.*)",
        "dest":"/public/img"
      }
    ]
  }
  